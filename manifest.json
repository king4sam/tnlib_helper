{
  "name": "tnlib_helper",
  "version": "0.3",
  "description": "Helper extension for Tainan Public Libraries",
  "permissions": ["activeTab", "declarativeContent","tabs","storage","http://163.26.71.106/","http://163.26.71.107/"],
  "content_scripts": [
    {
      "matches": ["http://163.26.71.107/*"],
      "js": ["script/lib/jquery-3.3.1.min.js", "script/myscript.js"],
      "run_at":"document_end"
    }
    ,{
      "matches": ["http://anding-lib.tainan.gov.tw/*"],
      "js": ["script/lib/jquery-3.3.1.min.js", "script/search_helper.js","script/hightlight.js"]
    }
  ],
  "background": {
    "scripts": ["script/background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
